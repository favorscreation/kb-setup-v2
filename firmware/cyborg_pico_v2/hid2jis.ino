String bit2mod(uint8_t bit) {
  String Result = "";
  if (bit & 0b00000001)
    Result += "LCTRL ";
  if (bit & 0b00000010)
    Result += "LSHIFT ";
  if (bit & 0b00000100)
    Result += "LALT ";
  if (bit & 0b00001000)
    Result += "LGUI ";
  if (bit & 0b00010000)
    Result += "RCTRL ";
  if (bit & 0b00100000)
    Result += "RSHIFT ";
  if (bit & 0b01000000)
    Result += "RALT ";
  if (bit & 0b10000000)
    Result += "RGUI ";

  return Result;
}

